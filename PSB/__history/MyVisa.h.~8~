//---------------------------------------------------------------------------

#ifndef MyVisaH
#define MyVisaH
//---------------------------------------------------------------------------
#include "visa.h"

/*
*程控电源切换输出43V和160V电压。

*/
class MyVisa
{
public:
	MyVisa(){}
	~MyVisa(){
		if(vi != NULL) viClose(vi); /*关闭仪器句柄和VISA资源管理器*/
		if(defaultRM != NULL) viClose(defaultRM);
	}
	bool __fastcall Initialization();
	void __fastcall SendAndRec();

private:
	ViSession vi;
	ViSession  defaultRM;
};





#endif
