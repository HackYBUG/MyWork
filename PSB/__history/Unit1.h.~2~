//---------------------------------------------------------------------------

#ifndef Unit1H
#define Unit1H
//---------------------------------------------------------------------------
enum RBTColor{RED,BLACK};

template<class T>
class RBTNode{
	public:
		RBTColor color;		//颜色
		T	key;            //关键字
		RBTNode *left; 		//左孩子
		RBTNode *right;		//右孩子
		RBTNode *parent;	//父节点

		RBTNode(T value, RBTNode c,RBTNode *p, RBTNode *l,RBTNode *r)"
};




#endif
